{
  "name": "<%= nameNormalized %>",
  "author": "<%= author %>",
  "description": "<%= name %> Documentation",
  "license": "MIT",
  "scripts": {
    "start": "node <%= src %>/config/start/serve.dev.js",
    "prebuild": "rimraf <%= output %>/",
    "build": "NODE_ENV=production webpack -p --progress --config <%= src %>/config/build/build.prod.js --profile --colors",
    "build:serve": "npm-run-all build serve:prod",
    "serve:prod": "http-server <%= output %> -o"
  },
  "dependencies": {
    "highlightjs": "9.8.0"
  },
  "devDependencies": {
    "http-server": "0.9.0",
    "npm-run-all": "4.0.0",
    "rimraf": "2.5.4",
    "swanky": "2.4.5",
    "webpack": "^2.2.0-rc"
  }
}